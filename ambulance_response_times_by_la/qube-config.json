        },
        "LGD2014": {
            "type": "dimension",
            "label": "Geography",
            "code_list": false,
            "cell_uri_template": "http://statistics.data.gov.uk/id/statistical-geography/{+lgd2014}",
            "from_existing": "http://purl.org/linked-data/sdmx/2009/dimension#refArea"
        },
        "Local Government District": false,
        "VALUE": {
            "type": "observations",
            "unit": {
                "label": "Minutes",
                "from_existing": "http://qudt.org/vocab/unit/MIN"
            }
        }
    }
}